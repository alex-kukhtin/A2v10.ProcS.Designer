<?xml version="1.0" encoding="utf-8" ?>
<mxEditor>
	<Array as="templates">
		<add as="Start">
			<Start hasLabel = "false">
				<mxCell vertex="1" style="shape=start;fillColor=#0691e5;strokeColor=#056fb0;resizable=0;">
					<mxGeometry as="geometry" width="40" height="40"/>
				</mxCell>
			</Start>
		</add>
		<add as="EndSuccess" >
			<EndSuccess hasLabel = "false">
				<mxCell vertex="1" style="shape=endSuccess;fillColor=#57b557;strokeColor=#3c7d3c;resizable=0;">
					<mxGeometry as="geometry" width="40" height="40"/>
				</mxCell>
			</EndSuccess>
		</add>
		<add as="EndError">
			<EndError hasLabel = "false">
				<mxCell vertex="1" style="shape=endError;fillColor=#f5644a;strokeColor=#ba4b38;resizable=0;">
					<mxGeometry as="geometry" width="40" height="40"/>
				</mxCell>
			</EndError>
		</add>
		<add as="State">
			<State Name="" Description="">
				<mxCell vertex="1"
						style="shape=swimlane;resizable=0;foldable=1;fillColor=azure;strokeColor=#999999;startSize=30;rounded=0;swimlaneFillColor=#ffffff">
					<mxGeometry as="geometry" width="200" height="70">
						<mxRectangle as="alternateBounds" width="200" height="70" />
					</mxGeometry>
				</mxCell>
				<properties>
					<Name type="input" label="Name"/>
					<Description type="textarea" label="Description"/>
				</properties>
			</State>
		</add>
		<add as="Transition">
			<Transition Name="" Condition="" Description="">
				<mxCell vertex="1" style="shape=rect;resizable=0;fillColor=#ffccff;strokeColor=#999999;movable=0;portConstraint=eastwest;">
					<mxGeometry as="geometry" width="180" height="30"/>
				</mxCell>
				<properties>
					<Name type="input" label="Name"/>
					<Condition type="textarea" label="Condition"/>
					<Description type="textarea" label="Description"/>
				</properties>
			</Transition>
		</add>
		<add as="Entry">
			<Entry>
				<mxCell vertex="1" style="shape=swimlane;resizable=0;fillColor=#23dd9050;strokeColor=#999999;movable=0;startSize=30;foldable=1;swimlaneFillColor=#fffff8;">
					<mxGeometry as="geometry" width="180" height="30"/>
				</mxCell>
			</Entry>
		</add>
		<add as="Code">
			<Code Code="" Description="">
				<mxCell vertex="1" style="shape=rect;resizable=0;fillColor=#f8dd8090;strokeColor=#999999;movable=0;">
					<mxGeometry as="geometry" width="0" height="30"/>
				</mxCell>
				<properties>
					<Code type="textarea" label="Code"/>
					<Description type="textarea" label="Description"/>
				</properties>
			</Code>
		</add>
		<add as="MyActivity">
			<MyActivity Code="">
				<mxCell vertex="1" style="shape=rect;resizable=0;fillColor=#98dd99;strokeColor=#999999;movable=0;">
					<mxGeometry as="geometry" width="150" height="30"/>
				</mxCell>
				<properties>
					<Code type="textarea" label="Code"/>
					<Memo type="textarea" label="Memo"/>
				</properties>
			</MyActivity>
		</add>
		<add as="CallHttpApiActivity">
			<CallHttpApiActivity Url="" Body="" Description="">
				<mxCell vertex="1" style="shape=rect;resizable=0;fillColor=#f3fdc9;strokeColor=#999999;movable=0;">
					<mxGeometry as="geometry" width="150" height="30"/>
				</mxCell>
				<properties>
					<Url type="input" label="Url"/>
					<Body type="textarea" label="Body"/>
					<Description type="textarea" label="Description"/>
				</properties>
			</CallHttpApiActivity>
		</add>
		<add as="WaitResumeActivity">
			<WaitResumeActivity Description="" Bookmark="">
				<mxCell vertex="1" style="shape=rect;resizable=0;fillColor=#f8dd8090;strokeColor=#999999;movable=0;">
					<mxGeometry as="geometry" width="150" height="30"/>
				</mxCell>
				<properties>
					<Bookmark type="input" label="Bookmark"/>
					<Description type="textarea" label="Description"/>
				</properties>
			</WaitResumeActivity>
		</add>
		<add as="ExecuteSqlActivity">
			<ExecuteSqlActivity Procedure="" Description="">
				<mxCell vertex="1" style="shape=rect;resizable=0;fillColor=#f8dd8090;strokeColor=#999999;movable=0;">
					<mxGeometry as="geometry" width="0" height="30"/>
				</mxCell>
				<properties>
					<Procedure type="input" label="Procedure"/>
					<Description type="textarea" label="Description"/>
				</properties>
			</ExecuteSqlActivity>
		</add>
	</Array>
	<mxGraph as="graph">
		<mxStylesheet as="stylesheet">
			<add as="defaultEdge">
				<add as="shape" value="connector"/>
				<add as="elbow" value="vertical"/>
				<add as="fontSize" value="10"/>
				<add as="strokeColor" value="blue"/>
				<add as="strokeWidth" value="2" />
				<add as="rounded" value="1"/>
				<add as="edgeStyle" value="orthogonalEdgeStyle"/>
				<add as="endArrow" value="classic"/>
			</add>
		</mxStylesheet>
	</mxGraph>
	<mxDefaultKeyHandler as="keyHandler">
		<add as="90" control="1" action="undo"/>
		<add as="89" control="1" action="redo"/>
		<add as="46" action="delete" key="del"/>
		<add as="88" control="1" action="cut"/>
		<add as="67" control="1" action="copy"/>
		<add as="86" control="1" action="paste"/>
		<add as="107" action="zoomIn"/>
		<add as="109" action="zoomOut"/>
	</mxDefaultKeyHandler>
</mxEditor>

