<?xml version="1.0" encoding="utf-8" ?>
<mxEditor>
	<Array as="templates">
		<add as="Task">
			<Task label="Task label" description="task description">
				<mxCell vertex="1" style="rounded">
					<mxGeometry as="geometry" width="72" height="32"/>
				</mxCell>
			</Task>
		</add>
		<add as="Start">
			<Start>
				<mxCell vertex="1" style="shape=start;fillColor=#0691e5;strokeColor=#056fb0;resizable=0">
					<mxGeometry as="geometry" width="50" height="50"/>
				</mxCell>
			</Start>
		</add>
		<add as="EndSuccess">
			<EndSuccess>
				<mxCell vertex="1" style="shape=endSuccess;fillColor=#57b557;strokeColor=#3c7d3c;resizable=0">
					<mxGeometry as="geometry" width="50" height="50"/>
				</mxCell>
			</EndSuccess>
		</add>
		<add as="EndError">
			<EndSuccess>
				<mxCell vertex="1" style="shape=endError;fillColor=#f5644a;strokeColor=#ba4b38;resizable=0">
					<mxGeometry as="geometry" width="50" height="50"/>
				</mxCell>
			</EndSuccess>
		</add>
		<add as="Condition">
			<Condition insidetop="40">
				<mxCell vertex="1"
						style="shape=swimlane;resizable=0;foldable=0;fillColor=azure;startSize=30">
					<mxGeometry as="geometry" width="200" height="150" />
				</mxCell>
			</Condition>
		</add>
		<add as="Transition">
			<Transition>
				<mxCell vertex="1" style="shape=rect;resizable=0;fillColor=#ffccff;movable=0;">
					<mxGeometry as="geometry" width="180" height="35"/>
				</mxCell>
			</Transition>
		</add>
	</Array>
	<mxDefaultKeyHandler as="keyHandler">
		<add as="90" control="1" action="undo"/>
		<add as="89" control="1" action="redo"/>
		<add as="46" action="delete" key="del"/>
		<add as="88" control="1" action="cut"/>
		<add as="67" control="1" action="copy"/>
		<add as="86" control="1" action="paste"/>
		<add as="107" action="zoomIn"/>
		<add as="109" action="zoomOut"/>
	</mxDefaultKeyHandler>
</mxEditor>

